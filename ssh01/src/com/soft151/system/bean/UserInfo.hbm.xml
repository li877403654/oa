<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<hibernate-mapping package="com.soft151.system.bean">		
		<class name="UserInfo" table="userinfo">
			<id name="userid" column="userid" type="java.lang.String"></id>
			<property name="username" type="java.lang.String">
				<column name="username"></column>
			</property>
			<property name="deptid" type="java.lang.Integer">
				<column name="deptid"></column>
			</property>
			<property name="password" type="java.lang.String">
				<column name="password"></column>
			</property>
			<property name="cellphone" type="java.lang.String">
				<column name="cellphone"></column>
			</property>
			<property name="usertype" type="java.lang.Integer">
				<column name="usertype"></column>
			</property>
		</class>
	</hibernate-mapping>	
